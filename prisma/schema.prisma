generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id          Int      @id @default(autoincrement()) 
  description String  
  amount      Decimal  
  type        String   
  account     String   
  category    String   
  date        DateTime @default(now()) 
}
